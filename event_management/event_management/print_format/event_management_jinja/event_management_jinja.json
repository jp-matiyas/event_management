 "html": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>{{ doc.event_name }} - Print</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            margin: 20px;\n            color: #333;\n        }\n        .container {\n            width: 80%;\n            margin: auto;\n        }\n        .header, .details, .attendees {\n            margin-bottom: 20px;\n        }\n        .header h2 {\n            margin: 0;\n            font-size: 24px;\n            color: #2c3e50;\n        }\n        .details p, .attendees p {\n            margin: 5px 0;\n            font-size: 16px;\n        }\n        .details p strong {\n            color: #34495e;\n        }\n        table {\n            width: 100%;\n            border-collapse: collapse;\n            margin-top: 10px;\n        }\n        table, th, td {\n            border: 2px solid black;\n        }\n        th, td {\n            padding: 8px;\n            text-align: left;\n        }\n        th {\n            background-color: #f2f2f2;\n            color: #2c3e50;\n        }\n        tr:nth-child(even) {\n            background-color: #f9f9f9;\n        }\n        .footer {\n            margin-top: 20px;\n            font-size: 14px;\n            text-align: center;\n            color: #888;\n        }\n    </style>\n</head>\n<body>\n    <div class=\"container\">\n         <div class=\"attendees\">\n            <h3>Event details</h3>\n            <table>\n                <thead>\n                    <tr>\n                        <th>Event Name</th>\n                        <th>Venue</th>\n                        <th>Event Date</th>\n                    </tr>\n                </thead>\n                <tbody>\n                   \n                    <tr>\n                        <td>{{ doc.event_name }}</td>\n                        <td>{{ doc.venue }}</td>\n                        <td>{{ frappe.utils.formatdate(doc.event_date, 'dd MMMM YYYY') }}</td>\n                    </tr>\n                    \n                </tbody>\n            </table>\n        </div>\n        \n        <div class=\"details\">\n            <p><strong>Start Time:</strong> {{ doc.start_time }} | <strong>End Time:</strong> {{ doc.end_time }}</p>\n            <p><strong>Total Duration:</strong> {{ doc.duration }} </p>\n            <p><strong>Organizer:</strong> {{ doc.organizer }}</p>\n            <p><strong>Description:</strong></p>\n            <p>{{ doc.description }}</p>\n        </div>\n        <div class=\"attendees\">\n            <h3>Attendees:</h3>\n            <table>\n                <thead>\n                    <tr>\n                        <th>Name</th>\n                        <th>Email</th>\n                        <th>Phone</th>\n                    </tr>\n                </thead>\n                <tbody>\n                    {% for attendee in doc.attendees %}\n                    <tr>\n                        <td>{{ attendee.attendee_name }}</td>\n                        <td>{{ attendee.attendee_email }}</td>\n                        <td>{{ attendee.attendee_phone }}</td>\n                    </tr>\n                    {% endfor %}\n                </tbody>\n            </table>\n        </div>\n        <div class=\"footer\">\n            <p>Thank you for attending!</p>\n        </div>\n    </div>\n</body>\n</html>",
